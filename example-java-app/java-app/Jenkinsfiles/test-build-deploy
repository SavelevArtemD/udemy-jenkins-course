pipeline {

    agent any
    tools {
        maven 'maven-3.8.2'
        jdk 'JDK'
    }

    stages {
        stage('Test') {
            steps {
                sh 'cd example-java-app/java-app'
                sh 'ls'
                sh 'mvn -B -DskipTests clean package'
            }
        }
    }
}