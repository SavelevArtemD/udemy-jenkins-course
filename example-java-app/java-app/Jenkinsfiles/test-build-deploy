pipeline {

    agent any
    tools {
        maven 'maven-3.8.2'
        jdk 'JDK'
    }

    stages {
        stage('Test') {
            steps {
                dir('example-java-app/java-app') {
                      sh 'mvn -B -DskipTests clean package'
                    }
            }
        }
    }
}